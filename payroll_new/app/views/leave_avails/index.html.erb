  </tr>

<% @leave_avails.each do |leave_avail| %>
  <tr>
    <td><%= leave_avail.empID %></td>
    <td><%= leave_avail.leaveID %></td>
    <td><%= leave_avail.leaveDate %></td>
    <td><%= leave_avail.leaveTakenDayPeriod %></td>
    <td><%= link_to 'Show', leave_avail %></td>
    <td><%= link_to 'Edit', edit_leave_avail_path(leave_avail) %></td>
